<%= semantic_form_for([(admin_namespace? ? "admin" : nil), @resource]) do |f| %>

  <%= render 'form_fields', f: f %>
	
	<%= f.actions do %>
		<li class="action">
    	<%= f.submit %>
    <li>
    <li class="cancel">
      <%= link_to t('fishing_memories.back'), :back %>
    <li>
  <% end %>

<% end %>
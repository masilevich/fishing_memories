<%=f.inputs do %>
	<%=f.input :occured_at, required: true, as: :date_picker %>
	<%=f.input :tackles, collection: @tackles, input_html: { class: 'select2', style: 'width: 100%;',data: { placeholder: 'Введите название снасти' }} %>
	<%=f.input :ponds, collection: @ponds, input_html: { class: 'select2', style: 'width: 100%;',data: { placeholder: 'Введите название водоема' }} %>
	<%=f.input :description, as: :text, input_html: {:rows => 35} %>
<%end%>
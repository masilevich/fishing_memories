<%=f.inputs do %>
	<%=f.input :occured_at, required: true, as: :date_picker %>
	<%=f.input :tackles, collection: @tackles, input_html: { class: 'select2', data: { placeholder: t('js.placeholders.memory_tackles') }} %>
	<%=f.input :ponds, collection: @ponds, input_html: { class: 'select2',data: { placeholder: t('js.placeholders.memory_ponds') }} %>
	<%#=f.input :description, as: :text, input_html: {:rows => 35} %>
	<%= f.input :description, :as => :ckeditor, :input_html => {:ckeditor => { :height => 400 } } %>
<%end%>